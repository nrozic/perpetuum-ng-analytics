{
  "name": "perpetuum-ng-analytics",
  "description": "Google Analytics module for Angular. This module is SSR compatible and can be used in Angular Universal applications",
  "version": "2.0.1",
  "main": "index.js",
  "author": {
    "name": "Nikola Rožić",
    "email": "nikola@trilium.io",
    "url": "https://trilium.io/"
  },
  "license": "(GPL-3.0)",
  "keywords": [
    "angular",
    "SSR",
    "Angular Universal",
    "Google Analytics",
    "Google Analytics v4",
    "gtag.js Angular"
  ],
  "homepage": "https://github.com/nrozic/perpetuum-ng-analytics",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrozic/perpetuum-ng-analytics.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build perpetuum-analytics && npm run lib:package",
    "lib:package": "cd dist/perpetuum-analytics && npm pack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && prettier-stylelint --write -q 'src/**/*.{css,scss}'",
      "pre-push": "ng lint",
      "post-merge": "run-if-changed"
    }
  },
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.3",
    "@angular/common": "~13.3.3",
    "@angular/compiler": "~13.3.3",
    "@angular/core": "~13.3.3",
    "@angular/forms": "~13.3.3",
    "@angular/platform-browser": "~13.3.3",
    "@angular/platform-browser-dynamic": "~13.3.3",
    "@angular/router": "~13.3.3",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.3",
    "@angular/language-service": "~13.3.3",
    "@types/jasmine": "^4.0.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^17.0.25",
    "codelyzer": "^6.0.2",
    "husky": "^7.0.4",
    "jasmine-core": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^4.0.0",
    "ng-packagr": "^13.3.1",
    "prettier": "^2.6.2",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^3.1.3",
    "protractor": "~7.0.0",
    "stacktrace-js": "^2.0.2",
    "stylelint": "^14.7.1",
    "stylelint-order": "^5.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.46.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.6.3"
  }
}
