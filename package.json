{
    "name": "perpetuum-ng-analytics",
    "description": "Google Analytics module for Angular. This module is SSR compatible and can be used in Angular Universal applications",
    "version": "1.1.1",
    "main": "index.js",
    "author": {
        "name": "Nikola Rožić",
        "email": "nikola@trilium.io",
        "url": "https://trilium.io/"
    },
    "license": "(GPL-3.0)",
    "keywords": [
        "angular",
        "SSR",
        "Angular Universal",
        "Google Analytics",
        "gtag.js Angular"
    ],
    "homepage": "https://github.com/nrozic/perpetuum-ng-analytics",
    "repository": {
        "type": "git",
        "url": "https://github.com/nrozic/perpetuum-ng-analytics.git"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "lib:build": "ng build perpetuum-analytics",
        "lib:package": "cd dist/perpetuum-analytics && npm pack"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged && prettier-stylelint --write -q 'src/**/*.{css,scss}'",
            "pre-push": "ng lint",
            "post-merge": "run-if-changed"
        }
    },
    "run-if-changed": {
        "package-lock.json": "npm install --prefer-offline"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.14",
        "@angular/common": "~8.2.14",
        "@angular/compiler": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14",
        "rxjs": "~6.4.0",
        "tslib": "^1.11.0",
        "zone.js": "~0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.803.25",
        "@angular-devkit/build-ng-packagr": "~0.803.25",
        "@angular/cli": "~8.3.21",
        "@angular/compiler-cli": "~8.2.14",
        "@angular/language-service": "~8.2.14",
        "@types/jasmine": "~3.3.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "^5.0.0",
        "husky": "^4.2.3",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "ng-packagr": "^5.4.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "protractor": "~5.4.0",
        "stacktrace-js": "^2.0.2",
        "stylelint": "^13.2.0",
        "stylelint-order": "^4.0.0",
        "ts-node": "~7.0.0",
        "tsickle": "^0.37.0",
        "tslint": "~5.15.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "~3.5.3"
    }
}
