{
  "name": "perpetuum-ng-analytics",
  "description": "Google Analytics module for Angular. This module is SSR compatible and can be used in Angular Universal applications",
  "version": "1.1.4",
  "main": "index.js",
  "author": {
    "name": "Nikola Rožić",
    "email": "nikola@trilium.io",
    "url": "https://trilium.io/"
  },
  "license": "(GPL-3.0)",
  "keywords": [
    "angular",
    "SSR",
    "Angular Universal",
    "Google Analytics",
    "Google Analytics v4",
    "gtag.js Angular"
  ],
  "homepage": "https://github.com/nrozic/perpetuum-ng-analytics",
  "repository": {
    "type": "git",
    "url": "https://github.com/nrozic/perpetuum-ng-analytics.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lib:build": "ng build perpetuum-analytics && npm run lib:package",
    "lib:package": "cd dist/perpetuum-analytics && npm pack"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && prettier-stylelint --write -q 'src/**/*.{css,scss}'",
      "pre-push": "ng lint",
      "post-merge": "run-if-changed"
    }
  },
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.16",
    "@angular/common": "~12.2.16",
    "@angular/compiler": "~12.2.16",
    "@angular/core": "~12.2.16",
    "@angular/forms": "~12.2.16",
    "@angular/platform-browser": "~12.2.16",
    "@angular/platform-browser-dynamic": "~12.2.16",
    "@angular/router": "~12.2.16",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.17",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~12.2.17",
    "@angular/compiler-cli": "~12.2.16",
    "@angular/language-service": "~12.2.16",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^4.0.0",
    "ng-packagr": "^12.2.7",
    "prettier": "^1.19.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "stacktrace-js": "^2.0.2",
    "stylelint": "^13.2.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.3.5"
  }
}